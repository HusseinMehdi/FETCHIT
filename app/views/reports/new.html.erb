<%= render 'users/header' %>

<body>
    <%= form_with url: results_reports_path, method: :get, local: true do |form| %>
        <div>
            <%= form.label :start_date, 'Start Date' %>
            <%= form.date_field :start_date, required: true %>
        </div>
        <div>
            <%= form.label :end_date, 'End Date' %>
            <%= form.date_field :end_date, required: true %>
        </div>
        
        <%= form.submit "Generate Report", class: "btn btn-primary mt-3" %>
    <% end %>
</body>
